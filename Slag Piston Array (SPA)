  <script>
    // Visualization of the 3000m Slag Column
    const ctx = document.getElementById('spaDiagram').getContext('2d');
    ctx.fillStyle = "#800080"; // Purple Pride
    ctx.fillRect(150, 50, 100, 500); // The Shaft
    ctx.strokeText("Vacuum Piston: 50t Slag", 160, 100);
  </script>
</body>
</html>

