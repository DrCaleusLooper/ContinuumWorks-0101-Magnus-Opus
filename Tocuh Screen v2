<script>
    /* * Ford A.G.I. Touch Interface Script 
     * Designed for 20 inch cockpit integration.
     */
    const surface = document.getElementById('interactive-surface');
    
    surface.addEventListener('touchstart', (e) => {
        e.preventDefault();
        surface.innerHTML = "A.G.I. PROCESSING INPUT...";
        surface.style.borderColor = "#6a0dad";
    });

    surface.addEventListener('mousedown', () => {
        surface.innerHTML = "A.G.I. OPTIMIZING SYSTEM...";
        surface.style.background = "rgba(106, 13, 173, 0.3)";
    });

    surface.addEventListener('mouseup', () => {
        setTimeout(() => {
            surface.innerHTML = "SYSTEM READY: ENGINE OF COMMUNITY ACTIVE";
            surface.style.background = "rgba(106, 13, 173, 0.1)";
        }, 1000);
    });
</script>

</body>
</html>

