import hashlib
import time

class KineticSeed:
    def __init__(self, device_motion_data):
        # We capture the raw X Y Z coordinates of the physical shake
        self.raw_entropy = str(device_motion_data)
        self.timestamp = str(time.time_ns())

    def generate_iridium_key(self, master_hash):
        # The Kinetic Seed is salted with the SHA 512 master key
        combined_seed = master_hash + self.raw_entropy + self.timestamp
        # This creates the unique Iridium Handshake for this session
        return hashlib.sha512(combined_seed.encode()).hexdigest()

    def frequency_hop_pattern(self, iridium_key):
        # We derive the radio mesh hopping pattern from the kinetic key
        # This ensures the walkie talkie bands rotate every 60 seconds
        return [int(iridium_key[i:i+2], 16) % 1024 for i in range(0, 20, 2)]

