        """
        The 'Snap-Back' Protocol.
        If malice is detected, the packet is rejected and the sender is flagged.
        """
        score = self.calculate_malice_vector(data_packet)
        if score > self.malice_threshold:
            return f"ACCESS DENIED. Malice detected ({score}). Protocol 3 Active."
        else:
            return self.process_flow(data_packet)

    def process_flow(self, clean_data):
